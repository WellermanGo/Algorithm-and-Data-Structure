//动态链表基本操作
struct node
{
    int data;
    node* next;
};
int main()
{
    int n, m; cin >> n >> m;
    node* head, * now, * pre, * suc;          //头节点，现节点，前驱节点，后继节点
    head = new node; head->data = 1; head->next = nullptr;
    now = head;
    for (int i = 2; i <= n; i++)
    {
        suc = new node;
        suc->data = i;
        suc->next = nullptr;
        
        now->next = suc;
        now = suc;
    }
    now->next = head;
    return 0;
}















